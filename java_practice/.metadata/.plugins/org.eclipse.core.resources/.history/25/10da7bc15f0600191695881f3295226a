import java.util.Scanner;

public class Test2 {
	public static void main(String[]args) {
		Scanner input  = new Scanner(System.in);
		String str ;
		str =input.next();
		int i =0 ;
		int sum =0 ;
		for(i=1;i<=9;i++) {
			int a = str.charAt(i)-48;
			sum+= a*i;
		}
		
		int s = sum%11;
		if(s==10) {
			System.out.println(str+"X");
		}
		else {
			System.out.println(str+s);
		}
	}
}
